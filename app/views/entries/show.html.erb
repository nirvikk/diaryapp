<h2><%= @entry.title %></h2>
  <p><%= @entry.date %></p>
  <p><%= @entry.contents %></p>
<p><%= link_to("Edit this entry", edit_entry_path(@entry)) %></p>
<p><%= link_to 'Destroy', entry_path(@entry),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></p>
<br>
<p><%= link_to("Back", entries_path) %></p>
